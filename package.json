{
  "version": "1.1.0",
  "name": "gatsby-starter-lumen",
  "description": "Simple starter for Gatsby",
  "main": "n/a",
  "scripts": {
    "start": "node app/app.js",
    "develop": "node ./cfPort > ./PORT;npm run build;./node_modules/gatsby/bin/gatsby.js develop --port `cat ./PORT` &",
    "build": "npm run clean;./node_modules/gatsby/bin/gatsby.js build &",
    "clean:public": "rm -rf public",
    "clean": "npm run clean:public",
    "copy:files": "cp pages/*.{pdf,txt,png,jpg,jpeg,gif} public/",
    "copy": "npm run copy:files",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploygh": "npm run clean;./node_modules/gatsby/bin/gatsby.js build --prefix-links && gh-pages -d public",
    "deploy": "git add -A .;git commit -m `date +%Y%m%d%s`;git tag -a `date +%Y%m%d%s` -m `date +%Y%m%d%s`;git push --follow-tags"
  },
  "keywords": [
    "gatsby",
    "lumen",
    "starter"
  ],
  "author": "Alexander Shelepenok <a.shelepenok@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "cfenv": "^1.0.3",
    "classnames": "^2.1.2",
    "cluster": "^0.7.7",
    "front-matter": "^2.0.8",
    "gatsby": "^0.12.19",
    "history": "^2.1.1",
    "koa": "^1.2.4",
    "koa-bodyparser": "^2.2.0",
    "koa-error": "^2.1.0",
    "koa-logger": "^1.3.0",
    "koa-passport": "^1.3.0",
    "koa-router": "^5.4.0",
    "koa-static": "^2.0.0",
    "lodash": "^4.14.1",
    "markdown-it": "^6.0.2",
    "moment": "^2.14.1",
    "passport": "^0.3.2",
    "passport-http-bearer": "^1.0.1",
    "password": "^0.1.1",
    "react": "^15.3.0",
    "react-document-title": "^2.0.2",
    "react-dom": "^15.3.0",
    "react-ga": "^2.1.2",
    "react-router": "^2.6.1",
    "safe-access": "^0.1.0",
    "underscore.string": "^3.2.3"
  },
  "devDependencies": {
    "babel-core": "^6.11.4",
    "babel-loader": "^6.2.4",
    "babel-preset-react-hmre": "^1.1.1",
    "css-loader": "^0.23.1",
    "eslint": "^3.2.2",
    "eslint-config-airbnb": "^10.0.0",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "gh-pages": "^0.11.0",
    "image-webpack-loader": "^2.0.0",
    "null-loader": "^0.1.1",
    "postcss-loader": "^0.9.1",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "^1.2.10",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "autoprefixer": "^6.3.7",
    "file-loader": "^0.9.0",
    "rucksack-css": "^0.8.5",
    "lost": "^7.0.3"
  },
  "engines": {
    "node": "6.2.x",
    "npm": "3.9.x"
  }
}
